    {
      "fieldset": "info",
      "label": "Семейное положение",
      "inputs": [
        {
          "type": "select",
          "options": {
            "married": "женат/замужем",
            "civil-marriage": "в «гражданском браке»",
            "divorced": "разведён",
            "single": "холост",
            "widowed": "вдовец"
          },
          "id": "marital-status"
        }
      ]
    },
    {
      "fieldset": "info",
      "label": "Образование",
      "inputs": [
        {
          "type": "select",
          "options": {
            "higher": "высшее",
            "post-secondary-vocational": "среднее специальное",
            "secondary-school": "среднее",
            "student": "студент"
          },
          "id": "education"
        }
      ]
    },
    {
      "fieldset": "contacts",
      "label": "Моб. телефон",
      "inputs": [
        {
          "type": "tel",
          "id": "tel",
          "placeholder": "+7",
          "required": true
        }
      ]
    },
    {
      "fieldset": "contacts",
      "label": "Электронная почта",
      "inputs": [
        {
          "type": "email",
          "id": "email",
          "placeholder": "ivanov@gmail.com",
          "required": true
        }
      ]
    }
