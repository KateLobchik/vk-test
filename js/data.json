          "max": 20,
          "required": true,
          "changes": true,
          "latin": true
        }
      ]
    },
    {
      "fieldset": "name",
      "label": "Имя",
      "inputs": [
        {
          "type": "text",
          "id": "name",
          "placeholder": "Иван",
          "min": 1,
          "max": 20,
          "required": true,
          "changes": false,
          "latin": true
        }
      ]
    },
    {
      "fieldset": "name",
      "label": "Отчество",
      "inputs": [
        {
          "fieldset": "name",
          "type": "text",
          "id": "patronymic",
          "placeholder": "Иванович",
          "min": 1,
          "max": 20,
          "required": true,
          "latin": true
        }
      ]
    },
    {
      "fieldset": "info",
      "label": "Дата рождения",
      "inputs": [
        {
          "type": "number",
          "id": "birth-day",
